<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.mpkg.org/manufacturing#"
     xml:base="http://www.mpkg.org/manufacturing"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:manufacturing="http://www.mpkg.org/manufacturing#">
    
    <!-- ================================================================= -->
    <!-- Ontology Declaration - 本体声明 -->
    <!-- ================================================================= -->
    
    <owl:Ontology rdf:about="http://www.mpkg.org/manufacturing">
        <owl:versionIRI rdf:resource="http://www.mpkg.org/manufacturing/2.0.0"/>
        <rdfs:label xml:lang="en">Manufacturing Domain Ontology</rdfs:label>
        <rdfs:label xml:lang="zh">制造领域本体</rdfs:label>
        <rdfs:comment xml:lang="en">A comprehensive OWL ontology for manufacturing domain knowledge representation, covering 167 machining processes, equipment, materials, parameters, and quality aspects.</rdfs:comment>
        <rdfs:comment xml:lang="zh">制造领域知识表示的综合OWL本体，涵盖167种机械加工工艺、设备、材料、参数和质量方面。</rdfs:comment>
        <owl:versionInfo>2.0.0 - Enhanced version with complete manufacturing taxonomy</owl:versionInfo>
        <rdfs:seeAlso>schemas/example_schema.csv</rdfs:seeAlso>
    </owl:Ontology>
    
    <!-- Material Relations - 材料关系 -->
    <owl:ObjectProperty rdf:about="http://www.mpkg.org/manufacturing#hasMaterial">
        <rdfs:label xml:lang="en">has material</rdfs:label>
        <rdfs:label xml:lang="zh">材料</rdfs:label>
        <rdfs:comment xml:lang="en">The part is made of specified material, e.g., 'Titanium Part - has material - Ti-6Al-4V'</rdfs:comment>
        <rdfs:comment xml:lang="zh">主体零件由指定的材料构成-如"钛合金零件-材料-T-6A1-4V"</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.mpkg.org/manufacturing#Part"/>
        <rdfs:range rdf:resource="http://www.mpkg.org/manufacturing#Material"/>
    </owl:ObjectProperty>
    
    <!-- Processing Method Relations - 加工方法关系 -->
    <owl:ObjectProperty rdf:about="http://www.mpkg.org/manufacturing#hasProcessingMethod">
        <rdfs:label xml:lang="en">has processing method</rdfs:label>
        <rdfs:label xml:lang="zh">加工方法</rdfs:label>
        <rdfs:comment xml:lang="en">The part is processed by specified method</rdfs:comment>
        <rdfs:comment xml:lang="zh">主体零件通过指定的加工方法进行处理-如"精密轴套-加工方法-车削"</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.mpkg.org/manufacturing#Part"/>
        <rdfs:range rdf:resource="http://www.mpkg.org/manufacturing#Process"/>
    </owl:ObjectProperty>
    
    <!-- Process Sequence Relations - 工序关系 -->
    <owl:ObjectProperty rdf:about="http://www.mpkg.org/manufacturing#hasProcessSequence">
        <rdfs:label xml:lang="en">has process sequence</rdfs:label>
        <rdfs:label xml:lang="zh">加工工序</rdfs:label>
        <rdfs:comment xml:lang="en">The part follows specified process sequence</rdfs:comment>
        <rdfs:comment xml:lang="zh">主体零件按照指定的工序顺序进行加工-如"粗车-半精车-精车-磨削"</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.mpkg.org/manufacturing#Part"/>
        <rdfs:range rdf:resource="http://www.mpkg.org/manufacturing#ProcessSequence"/>
    </owl:ObjectProperty>
    
    <!-- Equipment Usage Relations - 设备使用关系 -->
    <owl:ObjectProperty rdf:about="http://www.mpkg.org/manufacturing#usesEquipment">
        <rdfs:label xml:lang="en">uses equipment</rdfs:label>
        <rdfs:label xml:lang="zh">使用设备</rdfs:label>
        <rdfs:comment xml:lang="en">The process uses specified equipment</rdfs:comment>
        <rdfs:comment xml:lang="zh">主体工艺使用指定的设备进行操作-如"粗车加工-使用设备-KBSZ车床840"</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.mpkg.org/manufacturing#Process"/>
        <rdfs:range rdf:resource="http://www.mpkg.org/manufacturing#Equipment"/>
    </owl:ObjectProperty>
    
    <!-- Tool Usage Relations - 刀具使用关系 -->
    <owl:ObjectProperty rdf:about="http://www.mpkg.org/manufacturing#usesTool">
        <rdfs:label xml:lang="en">uses tool</rdfs:label>
        <rdfs:label xml:lang="zh">使用刀具</rdfs:label>
        <rdfs:comment xml:lang="en">The process uses specified cutting tool</rdfs:comment>
        <rdfs:comment xml:lang="zh">主体实体在执行过程中需要或推荐采用客体实体作为其实施的必要工具</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.mpkg.org/manufacturing#Process"/>
        <rdfs:range rdf:resource="http://www.mpkg.org/manufacturing#Tool"/>
    </owl:ObjectProperty>
    
    <!-- Parameter Relations - 参数关系 -->
    <owl:ObjectProperty rdf:about="http://www.mpkg.org/manufacturing#hasSpindleSpeed">
        <rdfs:label xml:lang="en">has spindle speed</rdfs:label>
        <rdfs:label xml:lang="zh">主轴转速</rdfs:label>
        <rdfs:comment xml:lang="en">The spindle rotation speed during processing</rdfs:comment>
        <rdfs:comment xml:lang="zh">加工过程中主轴的旋转速度-如"粗车加工-主轴转速-800r/min"</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.mpkg.org/manufacturing#Process"/>
        <rdfs:range rdf:resource="http://www.mpkg.org/manufacturing#SpindleSpeed"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://www.mpkg.org/manufacturing#hasCuttingSpeed">
        <rdfs:label xml:lang="en">has cutting speed</rdfs:label>
        <rdfs:label xml:lang="zh">切削速度</rdfs:label>
        <rdfs:comment xml:lang="en">The linear speed of tool relative to workpiece</rdfs:comment>
        <rdfs:comment xml:lang="zh">加工过程中工具相对于工件的线速度-如"车削-切削速度-120m/min"</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.mpkg.org/manufacturing#Process"/>
        <rdfs:range rdf:resource="http://www.mpkg.org/manufacturing#CuttingSpeed"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://www.mpkg.org/manufacturing#hasFeedRate">
        <rdfs:label xml:lang="en">has feed rate</rdfs:label>
        <rdfs:label xml:lang="zh">进给量</rdfs:label>
        <rdfs:comment xml:lang="en">The feed distance per revolution</rdfs:comment>
        <rdfs:comment xml:lang="zh">加工过程中工具每转进给的距离-如"车削加工-进给量-0.1mm/r"</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.mpkg.org/manufacturing#Process"/>
        <rdfs:range rdf:resource="http://www.mpkg.org/manufacturing#FeedRate"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://www.mpkg.org/manufacturing#hasCoolingMethod">
        <rdfs:label xml:lang="en">has cooling method</rdfs:label>
        <rdfs:label xml:lang="zh">冷却方式</rdfs:label>
        <rdfs:comment xml:lang="en">The cooling method used in process</rdfs:comment>
        <rdfs:comment xml:lang="zh">主体工艺采用指定的冷却方式进行操作-如"车削加工-冷却方式-切削液"</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.mpkg.org/manufacturing#Process"/>
        <rdfs:range rdf:resource="http://www.mpkg.org/manufacturing#CoolingMethod"/>
    </owl:ObjectProperty>
    
    <!-- Personnel Relations - 人员关系 -->
    <owl:ObjectProperty rdf:about="http://www.mpkg.org/manufacturing#operatedBy">
        <rdfs:label xml:lang="en">operated by</rdfs:label>
        <rdfs:label xml:lang="zh">操作人员</rdfs:label>
        <rdfs:comment xml:lang="en">The equipment is operated by specified personnel</rdfs:comment>
        <rdfs:comment xml:lang="zh">指定人员负责操作特定设备或执行特定工艺-如"数控加工中心XH714-操作人员-刘思嫄"</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.mpkg.org/manufacturing#Equipment"/>
        <rdfs:range rdf:resource="http://www.mpkg.org/manufacturing#Operator"/>
    </owl:ObjectProperty>
    
    <!-- Location Relations - 位置关系 -->
    <owl:ObjectProperty rdf:about="http://www.mpkg.org/manufacturing#locatedIn">
        <rdfs:label xml:lang="en">located in</rdfs:label>
        <rdfs:label xml:lang="zh">所在位置</rdfs:label>
        <rdfs:comment xml:lang="en">The physical location of equipment</rdfs:comment>
        <rdfs:comment xml:lang="zh">设备的物理位置或所属区域-如"数控加工中心XH714-所在位置-C车间"</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.mpkg.org/manufacturing#Equipment"/>
        <rdfs:range rdf:resource="http://www.mpkg.org/manufacturing#Location"/>
    </owl:ObjectProperty>
    
    <!-- Process Taxonomy Relations - 工艺分类关系 -->
    <owl:ObjectProperty rdf:about="http://www.mpkg.org/manufacturing#belongsToProcess">
        <rdfs:label xml:lang="en">belongs to process</rdfs:label>
        <rdfs:label xml:lang="zh">属于（工艺）</rdfs:label>
        <rdfs:comment xml:lang="en">The subject is a specific instance or subtype of the object process category</rdfs:comment>
        <rdfs:comment xml:lang="zh">主体实体是客体实体所代表的工艺类别的一个具体实例或子类型</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2000/01/rdf-schema#subClassOf"/>
    </owl:ObjectProperty>
    
    <!-- Heat Treatment Relations - 热处理关系 -->
    <owl:ObjectProperty rdf:about="http://www.mpkg.org/manufacturing#hasHeatTreatmentMethod">
        <rdfs:label xml:lang="en">has heat treatment method</rdfs:label>
        <rdfs:label xml:lang="zh">热处理方式</rdfs:label>
        <rdfs:comment xml:lang="en">Heat treatment method applied to the part</rdfs:comment>
        <rdfs:comment xml:lang="zh">主体零件经过指定的热处理方式进行处理-如"主轴-热处理方式-调质处理"</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.mpkg.org/manufacturing#Part"/>
        <rdfs:range rdf:resource="http://www.mpkg.org/manufacturing#HeatTreatment"/>
    </owl:ObjectProperty>
    
    <!-- Application Relations - 应用关系 -->
    <owl:ObjectProperty rdf:about="http://www.mpkg.org/manufacturing#suitableFor">
        <rdfs:label xml:lang="en">suitable for</rdfs:label>
        <rdfs:label xml:lang="zh">适用于</rdfs:label>
        <rdfs:comment xml:lang="en">Equipment or process suitable for specific applications</rdfs:comment>
        <rdfs:comment xml:lang="zh">设备或工艺适合的加工对象或应用场景-如"数控加工中心XH714-适用于-型腔和表面加工"</rdfs:comment>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.mpkg.org/manufacturing#Equipment"/>
                    <rdf:Description rdf:about="http://www.mpkg.org/manufacturing#Process"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:ObjectProperty>
    
    <!-- Surface Quality Properties - 表面质量属性 -->
    <owl:DatatypeProperty rdf:about="http://www.mpkg.org/manufacturing#hasSurfaceRoughness">
        <rdfs:label xml:lang="en">has surface roughness</rdfs:label>
        <rdfs:label xml:lang="zh">表面粗糙度要求</rdfs:label>
        <rdfs:comment xml:lang="en">Surface quality requirement</rdfs:comment>
        <rdfs:comment xml:lang="zh">主体零件需要达到指定的表面质量要求-如"钛合金零件-表面粗糙度要求-Ra1.6"</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.mpkg.org/manufacturing#Part"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    
    <!-- Cutting Parameters - 切削参数 -->
    <owl:DatatypeProperty rdf:about="http://www.mpkg.org/manufacturing#hasCuttingDepth">
        <rdfs:label xml:lang="en">has cutting depth</rdfs:label>
        <rdfs:label xml:lang="zh">切削深度</rdfs:label>
        <rdfs:comment xml:lang="en">The cutting depth value in mm</rdfs:comment>
        <rdfs:comment xml:lang="zh">主体工艺的切削深度为指定的数值范围-如"粗车加工-切削深度-1mm"</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.mpkg.org/manufacturing#Process"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://www.mpkg.org/manufacturing#hasAllowance">
        <rdfs:label xml:lang="en">has machining allowance</rdfs:label>
        <rdfs:label xml:lang="zh">加工余量</rdfs:label>
        <rdfs:comment xml:lang="en">Material allowance for next process in mm</rdfs:comment>
        <rdfs:comment xml:lang="zh">加工过程中为下道工序预留的材料量-如"粗车加工-加工余量-1.6mm"</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.mpkg.org/manufacturing#Process"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    </owl:DatatypeProperty>
    
    <!-- Material Properties - 材料特性 -->
    <owl:DatatypeProperty rdf:about="http://www.mpkg.org/manufacturing#hasHardnessRequirement">
        <rdfs:label xml:lang="en">has hardness requirement</rdfs:label>
        <rdfs:label xml:lang="zh">硬度要求</rdfs:label>
        <rdfs:comment xml:lang="en">Hardness standard requirement</rdfs:comment>
        <rdfs:comment xml:lang="zh">主体零件需要达到指定的硬度标准-如"调质处理-硬度要求-HRC30-35"</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.mpkg.org/manufacturing#Part"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    
    <!-- Dimensional Properties - 尺寸属性 -->
    <owl:DatatypeProperty rdf:about="http://www.mpkg.org/manufacturing#hasTolerance">
        <rdfs:label xml:lang="en">has tolerance</rdfs:label>
        <rdfs:label xml:lang="zh">目标公差</rdfs:label>
        <rdfs:comment xml:lang="en">Dimensional tolerance requirement</rdfs:comment>
        <rdfs:comment xml:lang="zh">主体零件需要达到的尺寸精度要求-如"主轴-目标公差-φ50h7"</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.mpkg.org/manufacturing#Part"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    
    <!-- Equipment Capabilities - 设备能力 -->
    <owl:DatatypeProperty rdf:about="http://www.mpkg.org/manufacturing#hasToolPositions">
        <rdfs:label xml:lang="en">has tool positions</rdfs:label>
        <rdfs:label xml:lang="zh">刀位数量</rdfs:label>
        <rdfs:comment xml:lang="en">Number of tool positions</rdfs:comment>
        <rdfs:comment xml:lang="zh">设备可容纳的刀具数量-如"数控加工中心XH714-刀位数量-16"</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.mpkg.org/manufacturing#Equipment"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://www.mpkg.org/manufacturing#hasMaxPrecision">
        <rdfs:label xml:lang="en">has maximum precision</rdfs:label>
        <rdfs:label xml:lang="zh">最高精度</rdfs:label>
        <rdfs:comment xml:lang="en">Maximum achievable precision</rdfs:comment>
        <rdfs:comment xml:lang="zh">设备能够达到的最高加工精度等级-如"数控加工中心XH714-最高精度-IT4等级"</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.mpkg.org/manufacturing#Equipment"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    
    <!-- Process Dimensions - 工序尺寸 -->
    <owl:DatatypeProperty rdf:about="http://www.mpkg.org/manufacturing#hasProcessDimension">
        <rdfs:label xml:lang="en">has process dimension</rdfs:label>
        <rdfs:label xml:lang="zh">工序尺寸</rdfs:label>
        <rdfs:comment xml:lang="en">Dimension to be achieved in this process</rdfs:comment>
        <rdfs:comment xml:lang="zh">某工序加工应达到的尺寸</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.mpkg.org/manufacturing#Process"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    
    <!-- Process Total Allowance - 加工总余量 -->
    <owl:DatatypeProperty rdf:about="http://www.mpkg.org/manufacturing#hasTotalAllowance">
        <rdfs:label xml:lang="en">has total allowance</rdfs:label>
        <rdfs:label xml:lang="zh">加工总余量</rdfs:label>
        <rdfs:comment xml:lang="en">Total material allowance from blank to finished part</rdfs:comment>
        <rdfs:comment xml:lang="zh">毛坯尺寸与零件图的设计尺寸之差</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.mpkg.org/manufacturing#Part"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    </owl:DatatypeProperty>
    
    <!-- Process Sequence Allowance - 工序余量 -->
    <owl:DatatypeProperty rdf:about="http://www.mpkg.org/manufacturing#hasSequenceAllowance">
        <rdfs:label xml:lang="en">has sequence allowance</rdfs:label>
        <rdfs:label xml:lang="zh">工序余量</rdfs:label>
        <rdfs:comment xml:lang="en">Allowance between adjacent processes</rdfs:comment>
        <rdfs:comment xml:lang="zh">相邻两工序的工艺尺寸之差</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.mpkg.org/manufacturing#ProcessSequence"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    </owl:DatatypeProperty>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#Entity">
        <rdfs:label xml:lang="en">Entity</rdfs:label>
        <rdfs:label xml:lang="zh">实体</rdfs:label>
        <rdfs:comment xml:lang="en">Base class for all manufacturing entities</rdfs:comment>
        <rdfs:comment xml:lang="zh">所有制造实体的基类</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#Process">
        <rdfs:label xml:lang="en">Process</rdfs:label>
        <rdfs:label xml:lang="zh">工艺</rdfs:label>
        <rdfs:comment xml:lang="en">Manufacturing processes and operations</rdfs:comment>
        <rdfs:comment xml:lang="zh">制造工艺和操作</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#Parameter">
        <rdfs:label xml:lang="en">Parameter</rdfs:label>
        <rdfs:label xml:lang="zh">参数</rdfs:label>
        <rdfs:comment xml:lang="en">Process parameters and specifications</rdfs:comment>
        <rdfs:comment xml:lang="zh">工艺参数和规格</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#QualityDefect">
        <rdfs:label xml:lang="en">Quality Defect</rdfs:label>
        <rdfs:label xml:lang="zh">质量缺陷</rdfs:label>
        <rdfs:comment xml:lang="en">Manufacturing quality issues and defects</rdfs:comment>
        <rdfs:comment xml:lang="zh">制造质量问题和缺陷</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#DimensionalChain">
        <rdfs:label xml:lang="en">Dimensional Chain</rdfs:label>
        <rdfs:label xml:lang="zh">尺寸链</rdfs:label>
        <rdfs:comment xml:lang="en">Related dimensional specifications</rdfs:comment>
        <rdfs:comment xml:lang="zh">互相联系且按一定顺序排列的封闭尺寸组合</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#Material">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#Entity"/>
        <rdfs:label xml:lang="en">Material</rdfs:label>
        <rdfs:label xml:lang="zh">材料</rdfs:label>
        <rdfs:comment xml:lang="en">Manufacturing materials</rdfs:comment>
        <rdfs:comment xml:lang="zh">制造材料</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#Equipment">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#Entity"/>
        <rdfs:label xml:lang="en">Equipment</rdfs:label>
        <rdfs:label xml:lang="zh">设备</rdfs:label>
        <rdfs:comment xml:lang="en">Manufacturing equipment and machines</rdfs:comment>
        <rdfs:comment xml:lang="zh">制造设备和机器</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#Tool">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#Entity"/>
        <rdfs:label xml:lang="en">Tool</rdfs:label>
        <rdfs:label xml:lang="zh">刀具</rdfs:label>
        <rdfs:comment xml:lang="en">Cutting tools and instruments</rdfs:comment>
        <rdfs:comment xml:lang="zh">切削刀具和工具</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#Part">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#Entity"/>
        <rdfs:label xml:lang="en">Part</rdfs:label>
        <rdfs:label xml:lang="zh">零件</rdfs:label>
        <rdfs:comment xml:lang="en">Manufactured parts and components</rdfs:comment>
        <rdfs:comment xml:lang="zh">制造零件和组件</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#Operator">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#Entity"/>
        <rdfs:label xml:lang="en">Operator</rdfs:label>
        <rdfs:label xml:lang="zh">操作人员</rdfs:label>
        <rdfs:comment xml:lang="en">Equipment operators and personnel</rdfs:comment>
        <rdfs:comment xml:lang="zh">设备操作员和人员</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#Location">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#Entity"/>
        <rdfs:label xml:lang="en">Location</rdfs:label>
        <rdfs:label xml:lang="zh">位置</rdfs:label>
        <rdfs:comment xml:lang="en">Physical locations and workshops</rdfs:comment>
        <rdfs:comment xml:lang="zh">物理位置和车间</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#MechanicalMachining">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#Process"/>
        <rdfs:label xml:lang="en">Mechanical Machining</rdfs:label>
        <rdfs:label xml:lang="zh">机械加工</rdfs:label>
        <rdfs:comment xml:lang="en">Machining processes using mechanical force</rdfs:comment>
        <rdfs:comment xml:lang="zh">利用机械力对各种工件进行加工的方法</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#PressureForming">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#Process"/>
        <rdfs:label xml:lang="en">Pressure Forming</rdfs:label>
        <rdfs:label xml:lang="zh">压力加工</rdfs:label>
        <rdfs:comment xml:lang="en">Forming processes using pressure without cutting</rdfs:comment>
        <rdfs:comment xml:lang="zh">使毛坯材料产生塑性变形或分离而无切削的加工方法</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#CuttingMachining">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#MechanicalMachining"/>
        <rdfs:label xml:lang="en">Cutting Machining</rdfs:label>
        <rdfs:label xml:lang="zh">切削加工</rdfs:label>
        <rdfs:comment xml:lang="en">Machining by removing material with cutting tools</rdfs:comment>
        <rdfs:comment xml:lang="zh">利用切削工具从工件上切除多余材料的加工方法</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#Turning">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#CuttingMachining"/>
        <rdfs:label xml:lang="en">Turning</rdfs:label>
        <rdfs:label xml:lang="zh">车削</rdfs:label>
        <rdfs:comment xml:lang="en">Workpiece rotates as main motion, tool feeds</rdfs:comment>
        <rdfs:comment xml:lang="zh">工件旋转作主运动.车刀作进给运动的切削加工方法</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#Milling">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#CuttingMachining"/>
        <rdfs:label xml:lang="en">Milling</rdfs:label>
        <rdfs:label xml:lang="zh">铣削</rdfs:label>
        <rdfs:comment xml:lang="en">Cutter rotates as main motion, workpiece or cutter feeds</rdfs:comment>
        <rdfs:comment xml:lang="zh">铣刀旋转作主运动.工件或铣刀作进给运动的切削加工方法</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#Planing">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#CuttingMachining"/>
        <rdfs:label xml:lang="en">Planing</rdfs:label>
        <rdfs:label xml:lang="zh">刨削</rdfs:label>
        <rdfs:comment xml:lang="en">Linear reciprocating cutting motion</rdfs:comment>
        <rdfs:comment xml:lang="zh">用刨刀对工件作水平相对直线往复运动的切削加工方法</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#Drilling">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#CuttingMachining"/>
        <rdfs:label xml:lang="en">Drilling</rdfs:label>
        <rdfs:label xml:lang="zh">钻削</rdfs:label>
        <rdfs:comment xml:lang="en">Making holes with drill bits</rdfs:comment>
        <rdfs:comment xml:lang="zh">用钻头或扩孔钻在工件上加工孔的方法</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#Reaming">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#CuttingMachining"/>
        <rdfs:label xml:lang="en">Reaming</rdfs:label>
        <rdfs:label xml:lang="zh">铰削</rdfs:label>
        <rdfs:comment xml:lang="en">Finishing holes with reamers</rdfs:comment>
        <rdfs:comment xml:lang="zh">用铰刀从工件孔壁上切除微量金属层.以提高其尺寸精度和降低表面粗糙度的方法</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#Countersinking">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#CuttingMachining"/>
        <rdfs:label xml:lang="en">Countersinking</rdfs:label>
        <rdfs:label xml:lang="zh">锪削</rdfs:label>
        <rdfs:comment xml:lang="en">Creating countersunk holes or flattening hole ends</rdfs:comment>
        <rdfs:comment xml:lang="zh">用锪钻或锪刀刮平孔的端面或切出沉孔的方法</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#Boring">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#CuttingMachining"/>
        <rdfs:label xml:lang="en">Boring</rdfs:label>
        <rdfs:label xml:lang="zh">镗削</rdfs:label>
        <rdfs:comment xml:lang="en">Boring tool rotates, workpiece or tool feeds</rdfs:comment>
        <rdfs:comment xml:lang="zh">镗刀旋转作主运动.工件或镗刀作进给运动的切削加工方法</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#Slotting">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#CuttingMachining"/>
        <rdfs:label xml:lang="en">Slotting</rdfs:label>
        <rdfs:label xml:lang="zh">插削</rdfs:label>
        <rdfs:comment xml:lang="en">Vertical reciprocating cutting motion</rdfs:comment>
        <rdfs:comment xml:lang="zh">用插刀对工件作垂直相对直线往复运动的切削加工方法</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#Broaching">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#CuttingMachining"/>
        <rdfs:label xml:lang="en">Broaching</rdfs:label>
        <rdfs:label xml:lang="zh">拉削</rdfs:label>
        <rdfs:comment xml:lang="en">Machining internal and external surfaces with broach</rdfs:comment>
        <rdfs:comment xml:lang="zh">用拉刀加工工件内、外表面的方法</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#Pushing">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#CuttingMachining"/>
        <rdfs:label xml:lang="en">Pushing</rdfs:label>
        <rdfs:label xml:lang="zh">推削</rdfs:label>
        <rdfs:comment xml:lang="en">Machining internal surfaces with push cutter</rdfs:comment>
        <rdfs:comment xml:lang="zh">用推刀加工工件内表面的方法</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#ReliefCutting">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#CuttingMachining"/>
        <rdfs:label xml:lang="en">Relief Cutting</rdfs:label>
        <rdfs:label xml:lang="zh">铲削</rdfs:label>
        <rdfs:comment xml:lang="en">Creating relief angles on toothed tools</rdfs:comment>
        <rdfs:comment xml:lang="zh">切出有关带齿工具的切削齿背以获得后面和后角的加工方法</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#Scraping">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#CuttingMachining"/>
        <rdfs:label xml:lang="en">Scraping</rdfs:label>
        <rdfs:label xml:lang="zh">刮削</rdfs:label>
        <rdfs:comment xml:lang="en">Removing thin surface layers with scraper</rdfs:comment>
        <rdfs:comment xml:lang="zh">用刮刀刮除工件表面薄层的加工方法</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#Grinding">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#CuttingMachining"/>
        <rdfs:label xml:lang="en">Grinding</rdfs:label>
        <rdfs:label xml:lang="zh">磨削</rdfs:label>
        <rdfs:comment xml:lang="en">Surface machining with abrasive wheels</rdfs:comment>
        <rdfs:comment xml:lang="zh">用磨具以较高的线速度对工件表面进行加工的方法</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#Honing">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#CuttingMachining"/>
        <rdfs:label xml:lang="en">Honing</rdfs:label>
        <rdfs:label xml:lang="zh">珩磨</rdfs:label>
        <rdfs:comment xml:lang="en">Precision finishing with honing tools</rdfs:comment>
        <rdfs:comment xml:lang="zh">利用珩磨工具对工件表面施加一定压力.珩磨工具同时作相对旋转和直线往复运动.切除工件上极小余量的精加工方法</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#SuperFinishing">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#CuttingMachining"/>
        <rdfs:label xml:lang="en">Super Finishing</rdfs:label>
        <rdfs:label xml:lang="zh">超精加工</rdfs:label>
        <rdfs:comment xml:lang="en">Ultra-precision finishing with fine abrasives</rdfs:comment>
        <rdfs:comment xml:lang="zh">用细粒度的磨具对工件施加很小的压力.并作往复振动和慢速纵向进给运动.以实现微量磨削的一种光整加工方法</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#Polishing">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#Process"/>
        <rdfs:label xml:lang="en">Polishing</rdfs:label>
        <rdfs:label xml:lang="zh">抛光</rdfs:label>
        <rdfs:comment xml:lang="en">Surface finishing for smooth, bright surfaces</rdfs:comment>
        <rdfs:comment xml:lang="zh">利用机械、化学或电化学的作用、使工件获得光亮、平整表面的加工方法</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#Extruding">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#PressureForming"/>
        <rdfs:label xml:lang="en">Extruding</rdfs:label>
        <rdfs:label xml:lang="zh">挤压</rdfs:label>
        <rdfs:comment xml:lang="en">Forming by applying pressure with extrusion tools</rdfs:comment>
        <rdfs:comment xml:lang="zh">用挤压工具以一定的压力作用于金属坯料或工件、使其产生塑性变形、从而将坯料成形或挤光工件表面的加工方法</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#Rolling">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#PressureForming"/>
        <rdfs:label xml:lang="en">Rolling</rdfs:label>
        <rdfs:label xml:lang="zh">滚压</rdfs:label>
        <rdfs:comment xml:lang="en">Forming by applying pressure with rolling tools</rdfs:comment>
        <rdfs:comment xml:lang="zh">用滚压工具对金属坯料或工件施加压力、使其产生塑性变形、从而将坯料成形或滚光工件表面的加工方法</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#ShotPeening">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#SurfaceTreatment"/>
        <rdfs:label xml:lang="en">Shot Peening</rdfs:label>
        <rdfs:label xml:lang="zh">喷丸</rdfs:label>
        <rdfs:comment xml:lang="en">Surface strengthening with high-speed pellets</rdfs:comment>
        <rdfs:comment xml:lang="zh">用小直径的弹丸、在压缩空气或离心力的作用下、高速喷射工件、进行表面强化和清理的加工方法</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#SandBlasting">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#SurfaceTreatment"/>
        <rdfs:label xml:lang="en">Sand Blasting</rdfs:label>
        <rdfs:label xml:lang="zh">喷砂</rdfs:label>
        <rdfs:comment xml:lang="en">Surface cleaning with high-speed sand particles</rdfs:comment>
        <rdfs:comment xml:lang="zh">用高速运行的砂粒喷射工件、进行表面清理、除锈或使其表面粗化的加工方法</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#ColdWorking">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#PressureForming"/>
        <rdfs:label xml:lang="en">Cold Working</rdfs:label>
        <rdfs:label xml:lang="zh">冷作</rdfs:label>
        <rdfs:comment xml:lang="en">Forming sheet metal without changing cross-section</rdfs:comment>
        <rdfs:comment xml:lang="zh">在基本不改变材料断面特征的情况下、将金属板材、型材等加工成各种制品的方法</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#Stamping">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#PressureForming"/>
        <rdfs:label xml:lang="en">Stamping</rdfs:label>
        <rdfs:label xml:lang="zh">冲压</rdfs:label>
        <rdfs:comment xml:lang="en">Forming sheet metal by separation or forming</rdfs:comment>
        <rdfs:comment xml:lang="zh">使板料经分离或成形而得到制件的加工方法</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#Riveting">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#Process"/>
        <rdfs:label xml:lang="en">Riveting</rdfs:label>
        <rdfs:label xml:lang="zh">铆接</rdfs:label>
        <rdfs:comment xml:lang="en">Non-detachable connection using rivets</rdfs:comment>
        <rdfs:comment xml:lang="zh">借助铆钉形成的不可拆连接</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#Casting">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#Process"/>
        <rdfs:label xml:lang="en">Casting</rdfs:label>
        <rdfs:label xml:lang="zh">铸造</rdfs:label>
        <rdfs:comment xml:lang="en">Forming parts by pouring molten metal into molds</rdfs:comment>
        <rdfs:comment xml:lang="zh">将熔融金属浇注、压射或吸入铸型型腔中.待其凝固后而得到一定形状和性能铸件的方法</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#Forging">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#PressureForming"/>
        <rdfs:label xml:lang="en">Forging</rdfs:label>
        <rdfs:label xml:lang="zh">锻造</rdfs:label>
        <rdfs:comment xml:lang="en">Shaping metal through plastic deformation</rdfs:comment>
        <rdfs:comment xml:lang="zh">在加工设备及工(模)具的作用下.使金属坯料或铸锭产生局部或全部的塑性变形.以获得一定几何形状、尺寸和质量的锻件的加工方法</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#HeatTreatment">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#Process"/>
        <rdfs:label xml:lang="en">Heat Treatment</rdfs:label>
        <rdfs:label xml:lang="zh">热处理</rdfs:label>
        <rdfs:comment xml:lang="en">Altering material properties through heating and cooling</rdfs:comment>
        <rdfs:comment xml:lang="zh">将固态金属或合金在一定介质中加热、保温和冷却.以改变其整体或表面组织.从而获得所需要性能的加工方法</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#SurfaceTreatment">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#Process"/>
        <rdfs:label xml:lang="en">Surface Treatment</rdfs:label>
        <rdfs:label xml:lang="zh">表面处理</rdfs:label>
        <rdfs:comment xml:lang="en">Modifying surface properties</rdfs:comment>
        <rdfs:comment xml:lang="zh">改变工件表面层的机械性能、物理性能或化学性能的加工方法</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#SurfaceCoating">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#SurfaceTreatment"/>
        <rdfs:label xml:lang="en">Surface Coating</rdfs:label>
        <rdfs:label xml:lang="zh">表面涂覆</rdfs:label>
        <rdfs:comment xml:lang="en">Forming coatings with specified materials</rdfs:comment>
        <rdfs:comment xml:lang="zh">用规定的异己材料.在工件表面上形成涂层的方法</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#PowderMetallurgy">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#Process"/>
        <rdfs:label xml:lang="en">Powder Metallurgy</rdfs:label>
        <rdfs:label xml:lang="zh">粉末冶金</rdfs:label>
        <rdfs:comment xml:lang="en">Forming products from metal powder</rdfs:comment>
        <rdfs:comment xml:lang="zh">将金属粉末(或与非金属粉末的混合物)压制成形和烧结等形成各种制品的方法</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#Bonding">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#Process"/>
        <rdfs:label xml:lang="en">Bonding</rdfs:label>
        <rdfs:label xml:lang="zh">粘接</rdfs:label>
        <rdfs:comment xml:lang="en">Connection using adhesives</rdfs:comment>
        <rdfs:comment xml:lang="zh">借助粘结剂形成的连接</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#BenchWork">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#Process"/>
        <rdfs:label xml:lang="en">Bench Work</rdfs:label>
        <rdfs:label xml:lang="zh">钳加工</rdfs:label>
        <rdfs:comment xml:lang="en">Manual work on bench with hand tools</rdfs:comment>
        <rdfs:comment xml:lang="zh">一般在钳台上以手工工具为主，对工件进行各种加工的方法</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#ElectricalMachining">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#Process"/>
        <rdfs:label xml:lang="en">Electrical Machining</rdfs:label>
        <rdfs:label xml:lang="zh">电加工</rdfs:label>
        <rdfs:comment xml:lang="en">Machining using electrical energy</rdfs:comment>
        <rdfs:comment xml:lang="zh">直接利用电能对工件进行加工的方法</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#EDM">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#ElectricalMachining"/>
        <rdfs:label xml:lang="en">EDM (Electrical Discharge Machining)</rdfs:label>
        <rdfs:label xml:lang="zh">电火花加工</rdfs:label>
        <rdfs:comment xml:lang="en">Machining by electrical discharge erosion</rdfs:comment>
        <rdfs:comment xml:lang="zh">在一定的介质中、通过工具电极之间的脉冲放电的电蚀作用、对工件进行加工的方法</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#ECM">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#ElectricalMachining"/>
        <rdfs:label xml:lang="en">ECM (Electrochemical Machining)</rdfs:label>
        <rdfs:label xml:lang="zh">电解加工</rdfs:label>
        <rdfs:comment xml:lang="en">Machining by anodic dissolution</rdfs:comment>
        <rdfs:comment xml:lang="zh">利用金属工件在电解液中所产生的阳极溶解作用、而进行加工的方法</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#ElectronBeamMachining">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#ElectricalMachining"/>
        <rdfs:label xml:lang="en">Electron Beam Machining</rdfs:label>
        <rdfs:label xml:lang="zh">电子束加工</rdfs:label>
        <rdfs:comment xml:lang="en">Machining with high-energy electron beam</rdfs:comment>
        <rdfs:comment xml:lang="zh">在真空条件下、利用电子枪中产生的电子经加速、聚焦，形成高能量大密度的细电子束以轰击工件被加工部位，使该部位的材料熔化和蒸发</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#IonBeamMachining">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#ElectricalMachining"/>
        <rdfs:label xml:lang="en">Ion Beam Machining</rdfs:label>
        <rdfs:label xml:lang="zh">离子束加工</rdfs:label>
        <rdfs:comment xml:lang="en">Machining with high-speed ion beam</rdfs:comment>
        <rdfs:comment xml:lang="zh">利用离子源产生的离子、在真空中经加速聚焦而形成高速高能的束状离子流、从而对工件进行加工的方法</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#PlasmaMachining">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#ElectricalMachining"/>
        <rdfs:label xml:lang="en">Plasma Machining</rdfs:label>
