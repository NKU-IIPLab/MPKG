        <rdfs:label xml:lang="zh">卷边</rdfs:label>
        <rdfs:comment xml:lang="en">Forming edges or hem sealing</rdfs:comment>
        <rdfs:comment xml:lang="zh">将板件边缘成形(或空心制件)的口部进行卷边或咬边的加工方法</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#EdgeMilling">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#Milling"/>
        <rdfs:label xml:lang="en">Edge Milling</rdfs:label>
        <rdfs:label xml:lang="zh">铣边</rdfs:label>
        <rdfs:comment xml:lang="en">Continuous cutting of sheet edges</rdfs:comment>
        <rdfs:comment xml:lang="zh">对板件的边缘进行连续的切削加工</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#Straightening">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#PressureForming"/>
        <rdfs:label xml:lang="en">Straightening</rdfs:label>
        <rdfs:label xml:lang="zh">矫正</rdfs:label>
        <rdfs:comment xml:lang="en">Removing bending and warping defects</rdfs:comment>
        <rdfs:comment xml:lang="zh">清除材料或制件的弯曲、翘曲、凸凹不平等缺陷的加工方法</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#Leveling">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#PressureForming"/>
