        <rdfs:label xml:lang="en">Paste Carburizing</rdfs:label>
        <rdfs:label xml:lang="zh">膏剂渗碳</rdfs:label>
        <rdfs:comment xml:lang="en">Carburizing with paste carburizer coating</rdfs:comment>
        <rdfs:comment xml:lang="zh">工件表面以膏状渗碳剂涂覆进行渗碳的工艺</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#SaltBathCarburizing">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#Carburizing"/>
        <rdfs:label xml:lang="en">Salt Bath Carburizing</rdfs:label>
        <rdfs:label xml:lang="zh">盐浴渗碳(液体渗碳)</rdfs:label>
        <rdfs:comment xml:lang="en">Carburizing in molten salt bath</rdfs:comment>
        <rdfs:comment xml:lang="zh">在熔融盐浴渗碳剂中进行渗碳的工艺</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#GasCarburizing">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#Carburizing"/>
        <rdfs:label xml:lang="en">Gas Carburizing</rdfs:label>
        <rdfs:label xml:lang="zh">气体渗碳</rdfs:label>
        <rdfs:comment xml:lang="en">Carburizing in gas carburizer atmosphere</rdfs:comment>
        <rdfs:comment xml:lang="zh">工件在气体渗碳剂中进行渗碳的工艺</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#DripCarburizing">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#GasCarburizing"/>
        <rdfs:label xml:lang="en">Drip Carburizing</rdfs:label>
        <rdfs:label xml:lang="zh">滴注式渗碳(滴液式渗碳)</rdfs:label>
        <rdfs:comment xml:lang="en">Gas carburizing by dripping liquid carburizer</rdfs:comment>
        <rdfs:comment xml:lang="zh">将苯、醇、煤油等液体渗碳剂直接滴入炉内裂解进行气体渗碳的工艺</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#IonCarburizing">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#Carburizing"/>
        <rdfs:label xml:lang="en">Ion Carburizing</rdfs:label>
        <rdfs:label xml:lang="zh">离子渗碳(辉光放电渗碳)</rdfs:label>
        <rdfs:comment xml:lang="en">Carburizing using glow discharge</rdfs:comment>
        <rdfs:comment xml:lang="zh">在低于一个大气压的渗碳气氛中，利用工件(阴极)和阳极之间产生的辉光放电进行渗碳的工艺</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#FluidizedBedCarburizing">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#Carburizing"/>
        <rdfs:label xml:lang="en">Fluidized Bed Carburizing</rdfs:label>
        <rdfs:label xml:lang="zh">流态床渗碳</rdfs:label>
        <rdfs:comment xml:lang="en">Carburizing in fluidized solid particles</rdfs:comment>
        <rdfs:comment xml:lang="zh">在悬浮于气流中形成流态化的固体颗粒渗碳介质中进行渗碳的工艺</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#ElectrolyticCarburizing">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#Carburizing"/>
        <rdfs:label xml:lang="en">Electrolytic Carburizing</rdfs:label>
        <rdfs:label xml:lang="zh">电解渗碳</rdfs:label>
        <rdfs:comment xml:lang="en">Carburizing by electrolysis</rdfs:comment>
        <rdfs:comment xml:lang="zh">在作为阴极的被处理件和在熔盐浴中的石墨阳极之间通以电流进行渗碳的工艺</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#VacuumCarburizing">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#Carburizing"/>
        <rdfs:label xml:lang="en">Vacuum Carburizing</rdfs:label>
        <rdfs:label xml:lang="zh">真空渗碳</rdfs:label>
        <rdfs:comment xml:lang="en">Carburizing under vacuum conditions</rdfs:comment>
        <rdfs:comment xml:lang="zh">在低于一个大气压的条件下进行气体渗碳的工艺</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#HighTemperatureCarburizing">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#Carburizing"/>
        <rdfs:label xml:lang="en">High Temperature Carburizing</rdfs:label>
        <rdfs:label xml:lang="zh">高温渗碳</rdfs:label>
        <rdfs:comment xml:lang="en">Carburizing above 950°C</rdfs:comment>
        <rdfs:comment xml:lang="zh">在950℃以上进行渗碳的工艺</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#LocalCarburizing">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#Carburizing"/>
        <rdfs:label xml:lang="en">Local Carburizing</rdfs:label>
        <rdfs:label xml:lang="zh">局部渗碳</rdfs:label>
        <rdfs:comment xml:lang="en">Carburizing only specific areas</rdfs:comment>
        <rdfs:comment xml:lang="zh">仅对工件表面某一部分或某些区域进行的渗碳</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#Recarburizing">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#Carburizing"/>
        <rdfs:label xml:lang="en">Recarburizing</rdfs:label>
        <rdfs:label xml:lang="zh">复碳</rdfs:label>
        <rdfs:comment xml:lang="en">Restoring carbon content after decarburization</rdfs:comment>
        <rdfs:comment xml:lang="zh">由于热处理或其他工序引起钢件表面脱碳后，为恢复初始碳含量而进行的渗碳处理</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#Nitriding">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#HeatTreatment"/>
        <rdfs:label xml:lang="en">Nitriding</rdfs:label>
        <rdfs:label xml:lang="zh">渗氮(氮化)</rdfs:label>
        <rdfs:comment xml:lang="en">Adding nitrogen to surface layer</rdfs:comment>
        <rdfs:comment xml:lang="zh">在一定温度下(一般在Ac1温度下)，使活性氮原子渗入工件表面的化学热处理工艺</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#LiquidNitriding">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#Nitriding"/>
        <rdfs:label xml:lang="en">Liquid Nitriding</rdfs:label>
        <rdfs:label xml:lang="zh">液体渗氮</rdfs:label>
        <rdfs:comment xml:lang="en">Nitriding in molten salt</rdfs:comment>
        <rdfs:comment xml:lang="zh">在熔盐渗氮剂中进行渗氮的工艺</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#GasNitriding">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#Nitriding"/>
        <rdfs:label xml:lang="en">Gas Nitriding</rdfs:label>
        <rdfs:label xml:lang="zh">气体渗氮</rdfs:label>
        <rdfs:comment xml:lang="en">Nitriding in gas atmosphere</rdfs:comment>
        <rdfs:comment xml:lang="zh">在气体介质中进行渗氮的工艺</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#IonNitriding">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#Nitriding"/>
        <rdfs:label xml:lang="en">Ion Nitriding</rdfs:label>
        <rdfs:label xml:lang="zh">离子渗氮(离子氮化)</rdfs:label>
        <rdfs:comment xml:lang="en">Nitriding using glow discharge</rdfs:comment>
        <rdfs:comment xml:lang="zh">在低于一个大气压的渗氮气氛中，利用工件(阴极)和阳极之间产生的辉光放电进行渗氮的工艺</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#SingleStageNitriding">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#Nitriding"/>
        <rdfs:label xml:lang="en">Single Stage Nitriding</rdfs:label>
        <rdfs:label xml:lang="zh">一段渗氮</rdfs:label>
        <rdfs:comment xml:lang="en">Nitriding at single temperature</rdfs:comment>
        <rdfs:comment xml:lang="zh">在一个温度下进行渗氮的工艺</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#MultiStageNitriding">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#Nitriding"/>
        <rdfs:label xml:lang="en">Multi-Stage Nitriding</rdfs:label>
        <rdfs:label xml:lang="zh">多段渗氮(多段氮化)</rdfs:label>
        <rdfs:comment xml:lang="en">Nitriding at multiple temperature stages</rdfs:comment>
        <rdfs:comment xml:lang="zh">将渗氮过程分在两个或三个温度阶段保温渗氮的工艺</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#Denitriding">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#HeatTreatment"/>
        <rdfs:label xml:lang="en">Denitriding</rdfs:label>
        <rdfs:label xml:lang="zh">退氮(脱氮)</rdfs:label>
        <rdfs:comment xml:lang="en">Removing excess nitrogen from surface</rdfs:comment>
        <rdfs:comment xml:lang="zh">从渗氮件表层去除过剩的氮进行的化学热处理工艺</rdfs:comment>
    </owl:Class>
    

    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#Overheating">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#QualityDefect"/>
        <rdfs:label xml:lang="en">Overheating</rdfs:label>
        <rdfs:label xml:lang="zh">过热</rdfs:label>
        <rdfs:comment xml:lang="en">Excessive grain growth due to high temperature</rdfs:comment>
        <rdfs:comment xml:lang="zh">金属或合金在热处理加热时，由于温度过高，晶粒长得很大，以致性能显著降低的现象</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#Segregation">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#QualityDefect"/>
        <rdfs:label xml:lang="en">Segregation</rdfs:label>
        <rdfs:label xml:lang="zh">偏析</rdfs:label>
        <rdfs:comment xml:lang="en">Non-uniform distribution in alloy</rdfs:comment>
        <rdfs:comment xml:lang="zh">合金中合金元素、夹杂物或气孔等分布不均匀的现象</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#ColdBrittleness">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#QualityDefect"/>
        <rdfs:label xml:lang="en">Cold Brittleness</rdfs:label>
        <rdfs:label xml:lang="zh">冷脆</rdfs:label>
        <rdfs:comment xml:lang="en">Reduced toughness at low temperature</rdfs:comment>
        <rdfs:comment xml:lang="zh">在低温(一般指100℃以下)，钢的冲击韧度随温度的降低而急剧下降的现象</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#BlueBrittleness">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#QualityDefect"/>
        <rdfs:label xml:lang="en">Blue Brittleness</rdfs:label>
        <rdfs:label xml:lang="zh">蓝脆</rdfs:label>
        <rdfs:comment xml:lang="en">Brittleness at 200-300°C</rdfs:comment>
        <rdfs:comment xml:lang="zh">钢在200-300℃(表面氧化膜呈蓝色)时抗拉强度及硬度比常温的高，塑性及韧度比常温的低的现象</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#HotBrittleness">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#QualityDefect"/>
        <rdfs:label xml:lang="en">Hot Brittleness</rdfs:label>
        <rdfs:label xml:lang="zh">热脆</rdfs:label>
        <rdfs:comment xml:lang="en">Grain boundary cracking near melting point</rdfs:comment>
        <rdfs:comment xml:lang="zh">有些合金在接近熔点的温度受到应力或形变时沿晶界开裂的现象</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#HydrogenEmbrittlement">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#QualityDefect"/>
        <rdfs:label xml:lang="en">Hydrogen Embrittlement</rdfs:label>
        <rdfs:label xml:lang="zh">氢脆</rdfs:label>
        <rdfs:comment xml:lang="en">Reduced toughness due to hydrogen absorption</rdfs:comment>
        <rdfs:comment xml:lang="zh">金属或合金因吸收氢而引起的韧度降低现象</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#WhiteSpot">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#QualityDefect"/>
        <rdfs:label xml:lang="en">White Spot</rdfs:label>
        <rdfs:label xml:lang="zh">白点</rdfs:label>
        <rdfs:comment xml:lang="en">Defect caused by hydrogen precipitation</rdfs:comment>
        <rdfs:comment xml:lang="zh">白点是钢中因氢的析出而引起的一种缺陷。在纵向断口上，它呈现接近圆形或椭圆形的银白色斑点;在侵蚀后的宏观磨片上呈现发裂</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#ProcessHole">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#Process"/>
        <rdfs:label xml:lang="en">Process Hole</rdfs:label>
        <rdfs:label xml:lang="zh">工艺孔</rdfs:label>
        <rdfs:comment xml:lang="en">Hole added for process requirements</rdfs:comment>
        <rdfs:comment xml:lang="zh">为满足工艺(加工、测量、装配)的需要而在工件上增设的孔</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#ProcessBoss">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#Process"/>
        <rdfs:label xml:lang="en">Process Boss</rdfs:label>
        <rdfs:label xml:lang="zh">工艺凸台</rdfs:label>
        <rdfs:comment xml:lang="en">Boss added for process requirements</rdfs:comment>
        <rdfs:comment xml:lang="zh">为满足工艺的需要而在工件上增设的凸台</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#ProcessDimension">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#DimensionalChain"/>
        <rdfs:label xml:lang="en">Process Dimension</rdfs:label>
        <rdfs:label xml:lang="zh">工艺尺寸</rdfs:label>
        <rdfs:comment xml:lang="en">Dimension specified for processing needs</rdfs:comment>
        <rdfs:comment xml:lang="zh">根据加工的需要、在工艺附图或工艺规程中所给出的尺寸</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#ProcessSequenceDimension">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#DimensionalChain"/>
        <rdfs:label xml:lang="en">Process Sequence Dimension</rdfs:label>
        <rdfs:label xml:lang="zh">工序尺寸</rdfs:label>
        <rdfs:comment xml:lang="en">Dimension to be achieved in specific process</rdfs:comment>
        <rdfs:comment xml:lang="zh">某工序加工应达到的尺寸</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#ProcessDimensionalChain">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#DimensionalChain"/>
        <rdfs:label xml:lang="en">Process Dimensional Chain</rdfs:label>
        <rdfs:label xml:lang="zh">工艺尺寸链</rdfs:label>
        <rdfs:comment xml:lang="en">Dimensional chain in processing</rdfs:comment>
        <rdfs:comment xml:lang="zh">在加工过程中的各有关工艺尺寸所组成的尺寸链</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#Derusting">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#SurfaceTreatment"/>
        <rdfs:label xml:lang="en">Derusting</rdfs:label>
        <rdfs:label xml:lang="zh">除锈</rdfs:label>
        <rdfs:comment xml:lang="en">Removing rust from surface</rdfs:comment>
        <rdfs:comment xml:lang="zh">将工件表面上的锈蚀除去的过程</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#Cleaning">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#Process"/>
        <rdfs:label xml:lang="en">Cleaning</rdfs:label>
        <rdfs:label xml:lang="zh">清洗</rdfs:label>
        <rdfs:comment xml:lang="en">Removing contaminants with cleaning agents</rdfs:comment>
        <rdfs:comment xml:lang="zh">用清洗剂清除产品或工件上的油污、灰尘等杂物的过程</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#Rounding">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#Process"/>
        <rdfs:label xml:lang="en">Rounding</rdfs:label>
        <rdfs:label xml:lang="zh">滚圆</rdfs:label>
        <rdfs:comment xml:lang="en">Forming rounded edges</rdfs:comment>
        <rdfs:comment xml:lang="zh">将工件边缘形成圆弧的加工方法</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#Assembly">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#Process"/>
        <rdfs:label xml:lang="en">Assembly</rdfs:label>
        <rdfs:label xml:lang="zh">装配</rdfs:label>
        <rdfs:comment xml:lang="en">Assembling parts into components</rdfs:comment>
        <rdfs:comment xml:lang="zh">把零件装配成部件的过程</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.mpkg.org/manufacturing#Hemming">
        <rdfs:subClassOf rdf:resource="http://www.mpkg.org/manufacturing#PressureForming"/>
