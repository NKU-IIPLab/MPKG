#################################################################
# General Axioms and Rules
#################################################################

# Disjoint Classes
DisjointClasses: Process, Entity, Parameter
DisjointClasses: Material, Equipment, Tool, Part
DisjointClasses: Turning, Milling, Grinding, Drilling
DisjointClasses: CuttingMachining, FormingProcess, HeatTreatment, WeldingAndJoining

# Property Chains
ObjectProperty: hasManufacturingProcess
    SubPropertyChain: hasMaterial o suitableFor o usesProcess

# Functional Properties
FunctionalProperty: operatedBy
FunctionalProperty: locatedIn

# Inverse Functional Properties
InverseFunctionalProperty: operates
    InverseOf: operatedBy

# Transitive Properties
TransitiveProperty: precedes
TransitiveProperty: follows

# Symmetric Properties
SymmetricProperty: isCompatibleWith

# Asymmetric Properties
AsymmetricProperty: precedes

# Reflexive Properties
ReflexiveProperty: isSimilarTo

# Irreflexive Properties
IrreflexiveProperty: precedes

#################################################################
# Class Restrictions and Axioms
#################################################################

Class: HighPrecisionProcess
    EquivalentTo: Process and (achievesQuality some (QualityLevel and (hasValue some xsd:string[pattern "IT[0-4]"])))
    
Class: RoughMachining
    EquivalentTo: Process and (hasDepthOfCut some (DepthOfCut and (hasValue some xsd:float[> 1.0])))
    
Class: FinishMachining
    EquivalentTo: Process and (hasDepthOfCut some (DepthOfCut and (hasValue some xsd:float[<= 0.5])))

Class: HardMachining
    EquivalentTo: Process and (appliedTo some (Part and (hasHardnessRequirement some xsd:string[pattern "HRC[4-9][0-9]"])))

Class: AutomatedProcess
    EquivalentTo: Process and (usesEquipment some CNCMachiningCenter)

Class: ManualProcess
    EquivalentTo: Process and not (usesEquipment some CNCMachiningCenter)

#################################################################
# SWRL Rules (Semantic Web Rule Language)
#################################################################

# Rule: If a process uses CBN tool, it should be for hard materials
Rule: Process(?p) ^ usesTool(?p, ?t) ^ CBNTool(?t) ^ hasMaterial(?part, ?m) ^ appliedTo(?p, ?part) 
      -> hasHardnessRequirement(?part, "HRC45+")

# Rule: High speed requires coolant
Rule: Process(?p) ^ hasCuttingSpeed(?p, ?s) ^ CuttingSpeed(?s) ^ hasValue(?s, ?v) ^ swrlb:greaterThan(?v, 200) 
      -> requiresCoolingMethod(?p, ?coolant) ^ Coolant(?coolant)

# Rule: Precision process requires finish machining
Rule: Part(?p) ^ hasToleranceGrade(?p, ?t) ^ ToleranceGrade(?t) ^ hasValue(?t, "IT6") 
      -> hasProcessingMethod(?p, ?fm) ^ FinishMachining(?fm)

# Rule: Titanium requires special tools
Rule: Process(?p) ^ appliedTo(?p, ?part) ^ hasMaterial(?part, ?m) ^ TitaniumAlloy(?m) 
      -> usesTool(?p, ?t) ^ (CBNTool(?t) or CarbideTool(?t))

