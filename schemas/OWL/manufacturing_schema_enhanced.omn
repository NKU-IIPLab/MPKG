Prefix: : <http://www.mpkg.org/manufacturing#>
Prefix: owl: <http://www.w3.org/2002/07/owl#>
Prefix: rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
Prefix: xml: <http://www.w3.org/XML/1998/namespace>
Prefix: xsd: <http://www.w3.org/2001/XMLSchema#>
Prefix: rdfs: <http://www.w3.org/2000/01/rdf-schema#>

Ontology: <http://www.mpkg.org/manufacturing>
<http://www.mpkg.org/manufacturing/1.0.0>

Annotations: 
    rdfs:label "Manufacturing Domain Ontology"@en,
    rdfs:label "制造领域本体"@zh,
    rdfs:comment "An OWL ontology for manufacturing domain knowledge representation, covering machining processes, equipment, materials, and parameters."@en,
    rdfs:comment "制造领域知识表示的OWL本体，涵盖机械加工工艺、设备、材料和参数。"@zh,
    owl:versionInfo "1.0.0"

ObjectProperty: hasMaterial
    Annotations: 
        rdfs:label "has material"@en,
        rdfs:label "材料"@zh,
        rdfs:comment "The part is made of specified material, e.g., 'Titanium Part - has material - Ti-6Al-4V'"@en,
        rdfs:comment "主体零件由指定的材料构成-如'钛合金零件-材料-T-6A1-4V'"@zh
    Domain: Part
    Range: Material

ObjectProperty: hasProcessingMethod
    Annotations: 
        rdfs:label "has processing method"@en,
        rdfs:label "加工方法"@zh,
        rdfs:comment "The part is processed by specified method"@en,
        rdfs:comment "主体零件通过指定的加工方法进行处理-如'精密轴套-加工方法-车削'"@zh
    Domain: Part
    Range: Process

ObjectProperty: hasProcessSequence
    Annotations: 
        rdfs:label "has process sequence"@en,
        rdfs:label "加工工序"@zh,
        rdfs:comment "The part follows specified process sequence"@en,
        rdfs:comment "主体零件按照指定的工序顺序进行加工-如'粗车-半精车-精车-磨削'"@zh
    Domain: Part
    Range: ProcessSequence

ObjectProperty: usesEquipment
    Annotations: 
        rdfs:label "uses equipment"@en,
        rdfs:label "使用设备"@zh,
        rdfs:comment "The process uses specified equipment"@en,
        rdfs:comment "主体工艺使用指定的设备进行操作-如'粗车加工-使用设备-KBSZ车床840'"@zh
    Domain: Process
    Range: Equipment

ObjectProperty: usesTool
    Annotations: 
        rdfs:label "uses tool"@en,
        rdfs:label "使用刀具"@zh,
        rdfs:comment "The process uses specified cutting tool"@en,
        rdfs:comment "主体实体在执行过程中需要或推荐采用客体实体作为其实施的必要工具"@zh
    Domain: Process
    Range: Tool

ObjectProperty: hasSpindleSpeed
    Annotations: 
        rdfs:label "has spindle speed"@en,
        rdfs:label "主轴转速"@zh,
        rdfs:comment "The spindle rotation speed during processing"@en,
        rdfs:comment "加工过程中主轴的旋转速度-如'粗车加工-主轴转速-800r/min'"@zh
    Domain: Process
    Range: SpindleSpeed

ObjectProperty: hasCuttingSpeed
    Annotations: 
        rdfs:label "has cutting speed"@en,
        rdfs:label "切削速度"@zh,
        rdfs:comment "The linear speed of tool relative to workpiece"@en,
        rdfs:comment "加工过程中工具相对于工件的线速度-如'车削-切削速度-120m/min'"@zh
    Domain: Process
    Range: CuttingSpeed

ObjectProperty: hasFeedRate
    Annotations: 
        rdfs:label "has feed rate"@en,
        rdfs:label "进给量"@zh,
        rdfs:comment "The feed distance per revolution"@en,
        rdfs:comment "加工过程中工具每转进给的距离-如'车削加工-进给量-0.1mm/r'"@zh
    Domain: Process
    Range: FeedRate

ObjectProperty: hasCoolingMethod
    Annotations: 
        rdfs:label "has cooling method"@en,
        rdfs:label "冷却方式"@zh,
        rdfs:comment "The cooling method used in process"@en,
        rdfs:comment "主体工艺采用指定的冷却方式进行操作-如'车削加工-冷却方式-切削液'"@zh
    Domain: Process
    Range: CoolingMethod

ObjectProperty: operatedBy
    Annotations: 
        rdfs:label "operated by"@en,
        rdfs:label "操作人员"@zh,
        rdfs:comment "The equipment is operated by specified personnel"@en,
        rdfs:comment "指定人员负责操作特定设备或执行特定工艺-如'数控加工中心XH714-操作人员-刘思嫄'"@zh
    Domain: Equipment
    Range: Operator

ObjectProperty: locatedIn
    Annotations: 
        rdfs:label "located in"@en,
        rdfs:label "所在位置"@zh,
        rdfs:comment "The physical location of equipment"@en,
        rdfs:comment "设备的物理位置或所属区域-如'数控加工中心XH714-所在位置-C车间'"@zh
    Domain: Equipment
    Range: Location

ObjectProperty: belongsToProcess
    Annotations: 
        rdfs:label "belongs to process"@en,
        rdfs:label "属于（工艺）"@zh,
        rdfs:comment "The subject is a specific instance or subtype of the object process category"@en,
        rdfs:comment "主体实体是客体实体所代表的工艺类别的一个具体实例或子类型"@zh
    SubPropertyOf: rdfs:subClassOf

ObjectProperty: hasHeatTreatmentMethod
    Annotations: 
        rdfs:label "has heat treatment method"@en,
        rdfs:label "热处理方式"@zh,
        rdfs:comment "The heat treatment method applied to the part"@en,
        rdfs:comment "主体零件经过指定的热处理方式进行处理-如'主轴-热处理方式-调质处理'"@zh
    Domain: Part
    Range: HeatTreatment

ObjectProperty: requiresCoolingMethod
    Annotations: 
        rdfs:label "requires cooling method"@en,
        rdfs:label "需要冷却方式"@zh,
        rdfs:comment "The cooling method required for the process"@en,
        rdfs:comment "加工过程需要的冷却方式"@zh
    Domain: Process
    Range: CoolingMethod

ObjectProperty: followsProcessSequence  
    Annotations: 
        rdfs:label "follows process sequence"@en,
        rdfs:label "遵循工序"@zh,
        rdfs:comment "The sequence of processes to be followed"@en,
        rdfs:comment "需要遵循的工序顺序"@zh
    Domain: Part
    Range: ProcessSequence

ObjectProperty: performedIn
    Annotations: 
        rdfs:label "performed in"@en,
        rdfs:label "执行于"@zh,
        rdfs:comment "Location where the process is performed"@en,
        rdfs:comment "工艺执行的位置"@zh
    Domain: Process
    Range: Location

ObjectProperty: producesDefect
    Annotations: 
        rdfs:label "produces defect"@en,
        rdfs:label "产生缺陷"@zh,
        rdfs:comment "Defect that may be produced by the process"@en,
        rdfs:comment "工艺可能产生的缺陷"@zh
    Domain: Process
    Range: Defect

ObjectProperty: preventedBy
    Annotations: 
        rdfs:label "prevented by"@en,
        rdfs:label "预防方法"@zh,
        rdfs:comment "Method to prevent the defect"@en,
        rdfs:comment "预防缺陷的方法"@zh
    Domain: Defect
    Range: Process

ObjectProperty: requiresTool
    Annotations: 
        rdfs:label "requires tool"@en,
        rdfs:label "需要刀具"@zh,
        rdfs:comment "Tool required for the process"@en,
        rdfs:comment "工艺需要的刀具"@zh
    Domain: Process
    Range: Tool

ObjectProperty: hasToolMaterial
    Annotations: 
        rdfs:label "has tool material"@en,
        rdfs:label "刀具材料"@zh,
        rdfs:comment "Material of the cutting tool"@en,
        rdfs:comment "切削刀具的材料"@zh
    Domain: Tool
    Range: Material

ObjectProperty: hasWorkpieceMaterial
    Annotations: 
        rdfs:label "has workpiece material"@en,
        rdfs:label "工件材料"@zh,
        rdfs:comment "Material of the workpiece"@en,
        rdfs:comment "工件的材料"@zh
    Domain: Part
    Range: Material

ObjectProperty: precedes
    Annotations: 
        rdfs:label "precedes"@en,
        rdfs:label "先于"@zh,
        rdfs:comment "Process that precedes another"@en,
        rdfs:comment "先于另一个工艺的工艺"@zh
    Domain: Process
    Range: Process
    Characteristics: Transitive

ObjectProperty: follows
    Annotations: 
        rdfs:label "follows"@en,
        rdfs:label "后于"@zh,
        rdfs:comment "Process that follows another"@en,
        rdfs:comment "后于另一个工艺的工艺"@zh
    Domain: Process
    Range: Process
    InverseOf: precedes

ObjectProperty: achievesQuality
    Annotations: 
        rdfs:label "achieves quality"@en,
        rdfs:label "达到质量"@zh,
        rdfs:comment "Quality level achieved by the process"@en,
        rdfs:comment "工艺达到的质量水平"@zh
    Domain: Process
    Range: QualityLevel

ObjectProperty: hasCapability
    Annotations: 
        rdfs:label "has capability"@en,
        rdfs:label "具有能力"@zh,
        rdfs:comment "Capability of the equipment"@en,
        rdfs:comment "设备的能力"@zh
    Domain: Equipment
    Range: Capability

ObjectProperty: appliedTo
    Annotations: 
        rdfs:label "applied to"@en,
        rdfs:label "应用于"@zh,
        rdfs:comment "Target of the process application"@en,
        rdfs:comment "工艺应用的目标"@zh
    Domain: Process
    Range: Part

ObjectProperty: usesFixture
    Annotations: 
        rdfs:label "uses fixture"@en,
        rdfs:label "使用夹具"@zh,
        rdfs:comment "Fixture used in the process"@en,
        rdfs:comment "工艺中使用的夹具"@zh
    Domain: Process
    Range: Fixture

ObjectProperty: hasMeasuringTool
    Annotations: 
        rdfs:label "has measuring tool"@en,
        rdfs:label "测量工具"@zh,
        rdfs:comment "Measuring tool used for inspection"@en,
        rdfs:comment "用于检验的测量工具"@zh
    Domain: Process
    Range: MeasuringTool

ObjectProperty: requiresSkill
    Annotations: 
        rdfs:label "requires skill"@en,
        rdfs:label "需要技能"@zh,
        rdfs:comment "Skill level required for the process"@en,
        rdfs:comment "工艺需要的技能水平"@zh
    Domain: Process
    Range: SkillLevel

ObjectProperty: hasProcessPlan
    Annotations: 
        rdfs:label "has process plan"@en,
        rdfs:label "工艺规程"@zh,
        rdfs:comment "Process plan for manufacturing"@en,
        rdfs:comment "制造的工艺规程"@zh
    Domain: Part
    Range: ProcessPlan

#################################################################
# Data Properties
#################################################################

DataProperty: hasProcessTime
    Annotations: 
        rdfs:label "has process time"@en,
        rdfs:label "加工时间"@zh,
        rdfs:comment "Time required for the process"@en,
        rdfs:comment "工艺所需的时间"@zh
    Domain: Process
    Range: xsd:float

DataProperty: hasCycleTime
    Annotations: 
        rdfs:label "has cycle time"@en,
        rdfs:label "循环时间"@zh,
        rdfs:comment "Cycle time for repetitive process"@en,
        rdfs:comment "重复工艺的循环时间"@zh
    Domain: Process
    Range: xsd:float

DataProperty: hasSetupTime
    Annotations: 
        rdfs:label "has setup time"@en,
        rdfs:label "调机时间"@zh,
        rdfs:comment "Setup time for equipment"@en,
        rdfs:comment "设备调试时间"@zh
    Domain: Equipment
    Range: xsd:float

DataProperty: hasToolLife
    Annotations: 
        rdfs:label "has tool life"@en,
        rdfs:label "刀具寿命"@zh,
        rdfs:comment "Expected tool life"@en,
        rdfs:comment "预期刀具寿命"@zh
    Domain: Tool
    Range: xsd:float

DataProperty: hasPowerConsumption
    Annotations: 
        rdfs:label "has power consumption"@en,
        rdfs:label "功率消耗"@zh,
        rdfs:comment "Power consumption of equipment"@en,
        rdfs:comment "设备的功率消耗"@zh
    Domain: Equipment
    Range: xsd:float

DataProperty: hasAccuracy
    Annotations: 
        rdfs:label "has accuracy"@en,
        rdfs:label "精度"@zh,
        rdfs:comment "Accuracy of the process or equipment"@en,
        rdfs:comment "工艺或设备的精度"@zh
    Domain: owl:Thing
    Range: xsd:string

DataProperty: hasRepeatability
    Annotations: 
        rdfs:label "has repeatability"@en,
        rdfs:label "重复精度"@zh,
        rdfs:comment "Repeatability of the process"@en,
        rdfs:comment "工艺的重复精度"@zh
    Domain: Process
    Range: xsd:float

DataProperty: hasCost
    Annotations: 
        rdfs:label "has cost"@en,
        rdfs:label "成本"@zh,
        rdfs:comment "Cost of the process or material"@en,
        rdfs:comment "工艺或材料的成本"@zh
    Domain: owl:Thing
    Range: xsd:float

DataProperty: hasTemperature
    Annotations: 
        rdfs:label "has temperature"@en,
        rdfs:label "温度"@zh,
        rdfs:comment "Operating temperature"@en,
        rdfs:comment "操作温度"@zh
    Domain: Process
    Range: xsd:float

DataProperty: hasPressure
    Annotations: 
        rdfs:label "has pressure"@en,
        rdfs:label "压力"@zh,
        rdfs:comment "Operating pressure"@en,
        rdfs:comment "操作压力"@zh
    Domain: Process
    Range: xsd:float

DataProperty: hasVibration
    Annotations: 
        rdfs:label "has vibration"@en,
        rdfs:label "振动"@zh,
        rdfs:comment "Vibration level"@en,
        rdfs:comment "振动水平"@zh
    Domain: Process
    Range: xsd:float

DataProperty: hasNoise
    Annotations: 
        rdfs:label "has noise level"@en,
        rdfs:label "噪音水平"@zh,
        rdfs:comment "Noise level in decibels"@en,
        rdfs:comment "以分贝为单位的噪音水平"@zh
    Domain: Equipment
    Range: xsd:float

DataProperty: hasEfficiency
    Annotations: 
        rdfs:label "has efficiency"@en,
        rdfs:label "效率"@zh,
        rdfs:comment "Process or equipment efficiency"@en,
        rdfs:comment "工艺或设备效率"@zh
    Domain: owl:Thing
    Range: xsd:float

DataProperty: hasQualityRate
    Annotations: 
        rdfs:label "has quality rate"@en,
        rdfs:label "合格率"@zh,
        rdfs:comment "Quality pass rate"@en,
        rdfs:comment "质量合格率"@zh
    Domain: Process
    Range: xsd:float

DataProperty: hasMaintenanceInterval
    Annotations: 
        rdfs:label "has maintenance interval"@en,
        rdfs:label "维护间隔"@zh,
        rdfs:comment "Maintenance interval in hours"@en,
        rdfs:comment "以小时为单位的维护间隔"@zh
    Domain: Equipment
    Range: xsd:float

DataProperty: hasWorkpieceSize
    Annotations: 
        rdfs:label "has workpiece size"@en,
        rdfs:label "工件尺寸"@zh,
        rdfs:comment "Maximum workpiece dimensions"@en,
        rdfs:comment "最大工件尺寸"@zh
    Domain: Equipment
    Range: xsd:string

DataProperty: hasBatchSize
    Annotations: 
        rdfs:label "has batch size"@en,
        rdfs:label "批量大小"@zh,
        rdfs:comment "Production batch size"@en,
        rdfs:comment "生产批量大小"@zh
    Domain: Process
    Range: xsd:integer

DataProperty: hasProcessCode
    Annotations: 
        rdfs:label "has process code"@en,
        rdfs:label "工艺代码"@zh,
        rdfs:comment "Process identification code"@en,
        rdfs:comment "工艺识别代码"@zh
    Domain: Process
    Range: xsd:string

DataProperty: hasMaterialGrade
    Annotations: 
        rdfs:label "has material grade"@en,
        rdfs:label "材料牌号"@zh,
        rdfs:comment "Material grade specification"@en,
        rdfs:comment "材料牌号规格"@zh
    Domain: Material
    Range: xsd:string

DataProperty: hasDensity
    Annotations: 
        rdfs:label "has density"@en,
        rdfs:label "密度"@zh,
        rdfs:comment "Material density"@en,
        rdfs:comment "材料密度"@zh
    Domain: Material
    Range: xsd:float

DataProperty: hasTensileStrength
    Annotations: 
        rdfs:label "has tensile strength"@en,
        rdfs:label "拉伸强度"@zh,
        rdfs:comment "Tensile strength of material"@en,
        rdfs:comment "材料的拉伸强度"@zh
    Domain: Material
    Range: xsd:float

DataProperty: hasYieldStrength
    Annotations: 
        rdfs:label "has yield strength"@en,
        rdfs:label "屈服强度"@zh,
        rdfs:comment "Yield strength of material"@en,
        rdfs:comment "材料的屈服强度"@zh
    Domain: Material
    Range: xsd:float

DataProperty: hasElongation
    Annotations: 
        rdfs:label "has elongation"@en,
        rdfs:label "延伸率"@zh,
        rdfs:comment "Material elongation percentage"@en,
        rdfs:comment "材料延伸率"@zh
    Domain: Material
    Range: xsd:float

DataProperty: hasCuttingDepth
    Annotations: 
        rdfs:label "has cutting depth"@en,
        rdfs:label "切削深度"@zh,
        rdfs:comment "The cutting depth value"@en,
        rdfs:comment "主体工艺的切削深度为指定的数值范围-如'粗车加工-切削深度-1mm'"@zh
    Domain: Process
    Range: xsd:float

DataProperty: hasAllowance
    Annotations: 
        rdfs:label "has machining allowance"@en,
        rdfs:label "加工余量"@zh,
        rdfs:comment "Material allowance for next process"@en,
        rdfs:comment "加工过程中为下道工序预留的材料量-如'粗车加工-加工余量-1.6mm'"@zh
    Domain: Process
    Range: xsd:float

DataProperty: hasHeatTreatmentMethod
    Annotations: 
        rdfs:label "has heat treatment method"@en,
        rdfs:label "热处理方式"@zh,
        rdfs:comment "Heat treatment method applied"@en,
        rdfs:comment "主体零件经过指定的热处理方式进行处理-如'主轴-热处理方式-调质处理'"@zh
    Domain: Part
    Range: xsd:string

DataProperty: hasHardnessRequirement
    Annotations: 
        rdfs:label "has hardness requirement"@en,
        rdfs:label "硬度要求"@zh,
        rdfs:comment "Hardness standard requirement"@en,
        rdfs:comment "主体零件需要达到指定的硬度标准-如'调质处理-硬度要求-HRC30-35'"@zh
    Domain: Part
    Range: xsd:string

DataProperty: hasTolerance
    Annotations: 
        rdfs:label "has tolerance"@en,
        rdfs:label "目标公差"@zh,
        rdfs:comment "Dimensional tolerance requirement"@en,
        rdfs:comment "主体零件需要达到的尺寸精度要求-如'主轴-目标公差-φ50h7'"@zh
    Domain: Part
    Range: xsd:string

DataProperty: hasToolPositions
    Annotations: 
        rdfs:label "has tool positions"@en,
        rdfs:label "刀位数量"@zh,
        rdfs:comment "Number of tool positions"@en,
        rdfs:comment "设备可容纳的刀具数量-如'数控加工中心XH714-刀位数量-16'"@zh
    Domain: Equipment
    Range: xsd:integer

DataProperty: suitableFor
    Annotations: 
        rdfs:label "suitable for"@en,
        rdfs:label "适用于"@zh,
        rdfs:comment "Suitable applications or scenarios"@en,
        rdfs:comment "设备或工艺适合的加工对象或应用场景-如'数控加工中心XH714-适用于-型腔和表面加工'"@zh
    Domain: Equipment
    Range: xsd:string

DataProperty: hasMaxPrecision
    Annotations: 
        rdfs:label "has maximum precision"@en,
        rdfs:label "最高精度"@zh,
        rdfs:comment "Maximum achievable precision"@en,
        rdfs:comment "设备能够达到的最高加工精度等级-如'数控加工中心XH714-最高精度-IT4等级'"@zh
    Domain: Equipment
    Range: xsd:string

#################################################################
# Classes
#################################################################

Class: Entity
    Annotations: 
        rdfs:label "Entity"@en,
        rdfs:label "实体"@zh,
        rdfs:comment "Base class representing all fundamental elements in the manufacturing domain including physical objects, processes, and abstract concepts"@en,
        rdfs:comment "所有制造实体的基类"@zh

Class: Process
    Annotations: 
        rdfs:label "Process"@en,
        rdfs:label "工艺"@zh,
        rdfs:comment "Manufacturing operations and procedures that transform raw materials into finished products through various methods and techniques"@en,
        rdfs:comment "制造工艺和操作"@zh

Class: Parameter
    Annotations: 
        rdfs:label "Parameter"@en,
        rdfs:label "参数"@zh,
        rdfs:comment "Quantifiable process variables and specifications that control and define manufacturing operations and their outcomes"@en,
        rdfs:comment "工艺参数和规格"@zh

Class: Material
    Annotations: 
        rdfs:label "Material"@en,
        rdfs:label "材料"@zh,
        rdfs:comment "Raw materials, alloys, and substances used in manufacturing processes with specific chemical compositions and mechanical properties"@en,
        rdfs:comment "制造材料"@zh
    SubClassOf: Entity

Class: Equipment
    Annotations: 
        rdfs:label "Equipment"@en,
        rdfs:label "设备"@zh,
        rdfs:comment "Manufacturing machines, tools, and devices used to perform processing operations and transform materials into products"@en,
        rdfs:comment "制造设备和机器"@zh
    SubClassOf: Entity

Class: Tool
    Annotations: 
        rdfs:label "Tool"@en,
        rdfs:label "刀具"@zh,
        rdfs:comment "Cutting implements and instruments that directly contact and remove material from workpieces during machining operations"@en,
        rdfs:comment "切削刀具和工具"@zh
    SubClassOf: Entity

Class: Part
    Annotations: 
        rdfs:label "Part"@en,
        rdfs:label "零件"@zh,
        rdfs:comment "Manufactured components and products resulting from processing operations, with specified dimensions, tolerances, and properties"@en,
        rdfs:comment "制造零件和组件"@zh
    SubClassOf: Entity

Class: Operator
    Annotations: 
        rdfs:label "Operator"@en,
        rdfs:label "操作人员"@zh,
        rdfs:comment "Skilled personnel responsible for operating equipment, executing processes, and ensuring quality in manufacturing operations"@en,
        rdfs:comment "设备操作员和人员"@zh
    SubClassOf: Entity

Class: Location
    Annotations: 
        rdfs:label "Location"@en,
        rdfs:label "位置"@zh,
        rdfs:comment "Physical areas within manufacturing facilities including workshops, production lines, and storage areas where operations occur"@en,
        rdfs:comment "物理位置和车间"@zh
    SubClassOf: Entity

Class: MechanicalMachining
    Annotations: 
        rdfs:label "Mechanical Machining"@en,
        rdfs:label "机械加工"@zh,
        rdfs:comment "Manufacturing processes that use mechanical force and cutting tools to shape materials by removing excess material"@en,
        rdfs:comment "利用机械力对各种工件进行加工的方法"@zh
    SubClassOf: Process

Class: PressureForming
    Annotations: 
        rdfs:label "Pressure Forming"@en,
        rdfs:label "压力加工"@zh,
        rdfs:comment "Manufacturing processes that shape materials through plastic deformation using pressure without material removal"@en,
        rdfs:comment "使毛坯材料产生塑性变形或分离而无切削的加工方法"@zh
    SubClassOf: Process

Class: CuttingMachining
    Annotations: 
        rdfs:label "Cutting Machining"@en,
        rdfs:label "切削加工"@zh,
        rdfs:comment "Material removal processes using sharp cutting tools to achieve desired shapes, dimensions, and surface finish"@en,
        rdfs:comment "利用切削工具从工件上切除多余材料的加工方法"@zh
    SubClassOf: MechanicalMachining

Class: Turning
    Annotations: 
        rdfs:label "Turning"@en,
        rdfs:label "车削"@zh,
        rdfs:comment "Machining process where workpiece rotates as primary motion while cutting tool feeds linearly to remove material and create cylindrical shapes"@en,
        rdfs:comment "工件旋转作主运动.车刀作进给运动的切削加工方法"@zh
    SubClassOf: CuttingMachining

Class: Milling
    Annotations: 
        rdfs:label "Milling"@en,
        rdfs:label "铣削"@zh,
        rdfs:comment "Machining process where rotating multi-tooth cutter removes material as workpiece or cutter feeds to create flat surfaces, slots, and complex contours"@en,
        rdfs:comment "铣刀旋转作主运动.工件或铣刀作进给运动的切削加工方法"@zh
    SubClassOf: CuttingMachining

Class: Planing
    Annotations: 
        rdfs:label "Planing"@en,
        rdfs:label "刨削"@zh,
        rdfs:comment "Machining process using linear reciprocating motion of cutting tool to create flat surfaces on large workpieces"@en,
        rdfs:comment "用刨刀对工件作水平相对直线往复运动的切削加工方法"@zh
    SubClassOf: CuttingMachining

Class: Drilling
    Annotations: 
        rdfs:label "Drilling"@en,
        rdfs:label "钻削"@zh,
        rdfs:comment "Machining process using rotating drill bits to create round holes in workpieces through axial feed motion"@en,
        rdfs:comment "用钻头或扩孔钻在工件上加工孔的方法"@zh
    SubClassOf: CuttingMachining

Class: Reaming
    Annotations: 
        rdfs:label "Reaming"@en,
        rdfs:label "铰削"@zh,
        rdfs:comment "Precision hole finishing process using multi-fluted reamers to remove small amounts of material for improved dimensional accuracy and surface finish"@en,
        rdfs:comment "用铰刀从工件孔壁上切除微量金属层.以提高其尺寸精度和降低表面粗糙度的方法"@zh
    SubClassOf: CuttingMachining

Class: Boring
    Annotations: 
        rdfs:label "Boring"@en,
        rdfs:label "镗削"@zh,
        rdfs:comment "Machining process for enlarging and finishing existing holes using rotating single-point boring tools"@en,
        rdfs:comment "镗刀旋转作主运动.工件或镗刀作进给运动的切削加工方法"@zh
    SubClassOf: CuttingMachining

Class: Grinding
    Annotations: 
        rdfs:label "Grinding"@en,
        rdfs:label "磨削"@zh,
        rdfs:comment "Precision machining process using abrasive wheels at high surface speeds to achieve fine surface finish and tight tolerances"@en,
        rdfs:comment "用磨具以较高的线速度对工件表面进行加工的方法"@zh
    SubClassOf: CuttingMachining

Class: Honing
    Annotations: 
        rdfs:label "Honing"@en,
        rdfs:label "珩磨"@zh,
        rdfs:comment "Ultra-precision finishing process using abrasive stones with combined rotary and reciprocating motion under controlled pressure"@en,
        rdfs:comment "利用珩磨工具对工件表面施加一定压力.珩磨工具同时作相对旋转和直线往复运动.切除工件上极小余量的精加工方法"@zh
    SubClassOf: CuttingMachining

Class: Polishing
    Annotations: 
        rdfs:label "Polishing"@en,
        rdfs:label "抛光"@zh,
        rdfs:comment "Surface finishing process using mechanical, chemical, or electrochemical methods to achieve mirror-like surface quality"@en,
        rdfs:comment "利用机械、化学或电化学的作用、使工件获得光亮、平整表面的加工方法"@zh
    SubClassOf: Process

Class: Casting
    Annotations: 
        rdfs:label "Casting"@en,
        rdfs:label "铸造"@zh,
        rdfs:comment "Manufacturing process where molten metal is poured, injected, or drawn into mold cavities and solidified to create parts with complex geometries"@en,
        rdfs:comment "将熔融金属浇注、压射或吸入铸型型腔中.待其凝固后而得到一定形状和性能铸件的方法"@zh
    SubClassOf: Process

Class: Forging
    Annotations: 
        rdfs:label "Forging"@en,
        rdfs:label "锻造"@zh,
        rdfs:comment "Manufacturing process using compressive forces to shape metal through controlled plastic deformation, improving grain structure and mechanical properties"@en,
        rdfs:comment "在加工设备及工(模)具的作用下.使金属坯料或铸锭产生局部或全部的塑性变形.以获得一定几何形状、尺寸和质量的锻件的加工方法"@zh
    SubClassOf: PressureForming

Class: HeatTreatment
    Annotations: 
        rdfs:label "Heat Treatment"@en,
        rdfs:label "热处理"@zh,
        rdfs:comment "Thermal processing of metals and alloys through controlled heating, holding, and cooling cycles to modify microstructure and achieve desired mechanical properties"@en,
        rdfs:comment "将固态金属或合金在一定介质中加热、保温和冷却.以改变其整体或表面组织.从而获得所需要性能的加工方法"@zh
    SubClassOf: Process

Class: SurfaceTreatment
    Annotations: 
        rdfs:label "Surface Treatment"@en,
        rdfs:label "表面处理"@zh,
        rdfs:comment "Processes that modify surface layer properties including hardness, wear resistance, corrosion resistance, and appearance"@en,
        rdfs:comment "改变工件表面层的机械性能、物理性能或化学性能的加工方法"@zh
    SubClassOf: Process

Class: Carburizing
    Annotations: 
        rdfs:label "Carburizing"@en,
        rdfs:label "渗碳"@zh,
        rdfs:comment "Thermochemical treatment process that introduces carbon atoms into steel surface layer to increase hardness and wear resistance while maintaining tough core"@en,
        rdfs:comment "向钢件表面层渗入碳原子的化学热处理工艺"@zh
    SubClassOf: HeatTreatment

Class: SpindleSpeed
    Annotations: 
        rdfs:label "Spindle Speed"@en,
        rdfs:label "主轴转速"@zh,
        rdfs:comment "Rotational velocity of machine tool spindle measured in revolutions per minute, determining cutting speed and surface finish quality"@en,
        rdfs:comment "主轴旋转速度，单位为转/分钟"@zh
    SubClassOf: Parameter

Class: CuttingSpeed
    Annotations: 
        rdfs:label "Cutting Speed"@en,
        rdfs:label "切削速度"@zh,
        rdfs:comment "Relative linear velocity between cutting tool and workpiece surface, critical for tool life and machining efficiency"@en,
        rdfs:comment "切削工具的线速度，单位为米/分钟"@zh
    SubClassOf: Parameter

Class: FeedRate
    Annotations: 
        rdfs:label "Feed Rate"@en,
        rdfs:label "进给量"@zh,
        rdfs:comment "Distance the cutting tool advances per workpiece revolution or per tool revolution, affecting chip formation and surface finish"@en,
        rdfs:comment "每转进给距离，单位为毫米/转"@zh
    SubClassOf: Parameter

Class: CoolingMethod
    Annotations: 
        rdfs:label "Cooling Method"@en,
        rdfs:label "冷却方式"@zh,
        rdfs:comment "Technique for removing heat and chips during machining, including flood cooling, mist cooling, and dry machining"@en,
        rdfs:comment "加工过程中的冷却方法"@zh
    SubClassOf: Parameter

Class: Lathe
    Annotations: 
        rdfs:label "Lathe"@en,
        rdfs:label "车床"@zh,
        rdfs:comment "Machine tool that rotates workpiece against cutting tools for turning, facing, boring, and threading operations"@en,
        rdfs:comment "用于车削加工的机床"@zh
    SubClassOf: Equipment

Class: MillingMachine
    Annotations: 
        rdfs:label "Milling Machine"@en,
        rdfs:label "铣床"@zh,
        rdfs:comment "Machine tool using rotating multi-tooth cutters to remove material from workpieces for creating flat and contoured surfaces"@en,
        rdfs:comment "用于铣削加工的机床"@zh
    SubClassOf: Equipment

Class: GrindingMachine
    Annotations: 
        rdfs:label "Grinding Machine"@en,
        rdfs:label "磨床"@zh,
        rdfs:comment "Precision machine tool using abrasive wheels for achieving high dimensional accuracy and superior surface finish"@en,
        rdfs:comment "用于磨削加工的机床"@zh
    SubClassOf: Equipment

Class: CNCMachiningCenter
    Annotations: 
        rdfs:label "CNC Machining Center"@en,
        rdfs:label "数控加工中心"@zh,
        rdfs:comment "Advanced computer-controlled machine tool capable of performing multiple machining operations with automatic tool changing"@en,
        rdfs:comment "计算机控制的多轴加工中心"@zh
    SubClassOf: Equipment

Class: CarbideTool
    Annotations: 
        rdfs:label "Carbide Tool"@en,
        rdfs:label "硬质合金刀具"@zh,
        rdfs:comment "High-performance cutting tool made from tungsten carbide particles bonded with cobalt, offering high hardness and wear resistance"@en,
        rdfs:comment "由硬质合金制成的切削刀具"@zh
    SubClassOf: Tool

Class: CBNTool
    Annotations: 
        rdfs:label "CBN Tool"@en,
        rdfs:label "CBN刀具"@zh,
        rdfs:comment "Ultra-hard cutting tool made from cubic boron nitride, second only to diamond in hardness, ideal for machining hardened steels"@en,
        rdfs:comment "立方氮化硼切削刀具"@zh
    SubClassOf: Tool

Class: TitaniumAlloy
    Annotations: 
        rdfs:label "Titanium Alloy"@en,
        rdfs:label "钛合金"@zh,
        rdfs:comment "High-strength, low-density alloys based on titanium with excellent corrosion resistance and high-temperature performance"@en,
        rdfs:comment "钛基合金材料"@zh
    SubClassOf: Material

Class: Steel
    Annotations: 
        rdfs:label "Steel"@en,
        rdfs:label "钢"@zh,
        rdfs:comment "Iron-carbon alloys with carbon content up to 2.14%, the most widely used engineering material in manufacturing"@en,
        rdfs:comment "铁碳合金材料"@zh
    SubClassOf: Material

Class: StainlessSteel
    Annotations: 
        rdfs:label "Stainless Steel"@en,
        rdfs:label "不锈钢"@zh,
        rdfs:comment "Steel alloys containing minimum 10.5% chromium providing excellent corrosion resistance and aesthetic appearance"@en,
        rdfs:comment "耐腐蚀钢合金"@zh
    SubClassOf: Steel

Class: ProcessSequence
    Annotations: 
        rdfs:label "Process Sequence"@en,
        rdfs:label "工序序列"@zh,
        rdfs:comment "Systematically arranged series of manufacturing operations performed in specific order to transform raw material into finished product"@en,
        rdfs:comment "制造工序的有序序列"@zh

Class: RoughTurning
    Annotations: 
        rdfs:label "Rough Turning"@en,
        rdfs:label "粗车"@zh,
        rdfs:comment "First stage turning operation removing large amounts of material quickly with emphasis on productivity over surface finish"@en,
        rdfs:comment "大余量去除的初始车削"@zh
    SubClassOf: Turning

Class: SemiFinishTurning
    Annotations: 
        rdfs:label "Semi-Finish Turning"@en,
        rdfs:label "半精车"@zh,
        rdfs:comment "Intermediate turning stage balancing material removal rate and dimensional accuracy, preparing for final finishing"@en,
        rdfs:comment "中间车削操作"@zh
    SubClassOf: Turning

Class: FinishTurning
    Annotations: 
        rdfs:label "Finish Turning"@en,
        rdfs:label "精车"@zh,
        rdfs:comment "Final turning operation with small depth of cut and fine feed to achieve required dimensions, tolerances, and surface finish"@en,
        rdfs:comment "用于精度和表面质量的最终车削"@zh
    SubClassOf: Turning

Class: ExternalTurning
    Annotations: 
        rdfs:label "External Turning"@en,
        rdfs:label "车外圆"@zh,
        rdfs:comment "Turning external cylindrical surfaces"@en,
        rdfs:comment "车削外圆柱面"@zh
    SubClassOf: Turning

Class: ExternalGrinding
    Annotations: 
        rdfs:label "External Grinding"@en,
        rdfs:label "磨外圆"@zh,
        rdfs:comment "Grinding external cylindrical surfaces"@en,
        rdfs:comment "磨削外圆柱面"@zh
    SubClassOf: Grinding

Class: FaceMilling
    Annotations: 
        rdfs:label "Face Milling"@en,
        rdfs:label "铣平面"@zh,
        rdfs:comment "Milling flat surfaces"@en,
        rdfs:comment "铣削平面"@zh
    SubClassOf: Milling

